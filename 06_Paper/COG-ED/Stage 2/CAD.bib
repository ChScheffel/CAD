  author     = {Strobel, Alexander and Wieder, Gesine and Paulus, Philipp C. and Ott, Florian and Pannasch, Sebastian and Kiebel, Stefan J. and Kührt, Corinna},
  journal    = {PLOS ONE},
  title      = {Dispositional cognitive effort investment and behavioral demand avoidance: {Are} they related?},
  year       = {2020},
  issn       = {1932-6203},
  month      = oct,
  note       = {Publisher: Public Library of Science},
  number     = {10},
  pages      = {e0239817},
  volume     = {15},
  abstract   = {Individuals tend to avoid cognitive demand, yet, individual differences appear to exist. Recent evidence from two studies suggests that individuals high in the personality traits Self-Control and Need for Cognition that are related to the broader construct Cognitive Effort Investment are less prone to avoid cognitive demand and show less effort discounting. These findings suggest that cost-benefit models of decision-making that integrate the costs due to effort should consider individual differences in the willingness to exert mental effort. However, to date, there are almost no replication attempts of the above findings. For the present conceptual replication, we concentrated on the avoidance of cognitive demand and used a longitudinal design and latent state-trait modeling. This approach enabled us to separate the trait-specific variance in our measures of Cognitive Effort Investment and Demand Avoidance that is due to stable, individual differences from the variance that is due to the measurement occasion, the methods used, and measurement error. Doing so allowed us to test the assumption that self-reported Cognitive Effort Investment is related to behavioral Demand Avoidance more directly by relating their trait-like features to each other. In a sample of N = 217 participants, we observed both self-reported Cognitive Effort Investment and behavioral Demand Avoidance to exhibit considerable portions of trait variance. However, these trait variances were not significantly related to each other. Thus, our results call into question previous findings of a relationship between self-reported effort investment and demand avoidance. We suggest that novel paradigms are needed to emulate real-world effortful situations and enable better mapping between self-reported measures and behavioral markers of the willingness to exert cognitive effort.},
  doi        = {10.1371/journal.pone.0239817},
  file       = {Full Text PDF:C\:/Users/josep/Zotero/storage/FFAQDW2F/strobel_dispositional_2020 - Dispositional Cognitive Effort Investment and Behavioral Demand Avoidance_ Are They Related_.pdf:application/pdf;Snapshot:C\:/Users/josep/Zotero/storage/2H465PAX/strobel_dispositional_2020 - Dispositional Cognitive Effort Investment and Behavioral Demand Avoidance_ Are They Related_.html:text/html},
  groups     = {CERED},
  keywords   = {Personality, Cognition, Behavior, Personality traits, Decision making, Working memory, Personality disorders, Personality tests},
  language   = {en},
  shorttitle = {Dispositional cognitive effort investment and behavioral demand avoidance},
  url        = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0239817},
  urldate    = {2021-07-14},
}

@Article{Westbrook2013,
  author     = {Westbrook, Andrew and Kester, Daria and Braver, Todd S.},
  journal    = {PLOS ONE},
  title      = {What is the subjective cost of cognitive effort? {Load}, trait, and aging effects revealed by economic preference},
  year       = {2013},
  issn       = {1932-6203},
  month      = jul,
  note       = {Publisher: Public Library of Science},
  number     = {7},
  pages      = {e68210},
  volume     = {8},
  abstract   = {It has long been assumed that people treat cognitive effort as costly, but also that such effort costs may vary greatly across individuals. Individual differences in subjective effort could present a major and pervasive confound in behavioral and neuroscience assessments, by conflating cognitive ability with cognitive motivation. Self-report cognitive effort scales have been developed, but objective measures are lacking. In this study, we use the behavioral economic approach of revealed preferences to quantify subjective effort. Specifically, we adapted a well-established discounting paradigm to measure the extent to which cognitive effort causes participants to discount monetary rewards. The resulting metrics are sensitive to both within-individual factors, including objective load and reward amount, and between-individual factors, including age and trait cognitive engagement. We further validate cognitive effort discounting by benchmarking it against well-established measures of delay discounting. The results highlight the promise and utility of behavioral economic tools for assessing trait and state influences on cognitive motivation.},
  doi        = {10.1371/journal.pone.0068210},
  file       = {Full Text PDF:C\:/Users/josep/Zotero/storage/ZA8ULIEI/westbrook_what_2013 - What Is the Subjective Cost of Cognitive Effort_ Load, Trait, and Aging Effects Revealed by Economic Preference.pdf:application/pdf;Snapshot:C\:/Users/josep/Zotero/storage/CY38ULMT/westbrook_what_2013 - What Is the Subjective Cost of Cognitive Effort_ Load, Trait, and Aging Effects Revealed by Economic Preference.html:text/html},
  groups     = {CERED},
  keywords   = {Motivation, Behavior, Decision making, Cognitive psychology, Measurement, Behavioral economics, Elderly, Payment},
  language   = {en},
  shorttitle = {What {Is} the {Subjective} {Cost} of {Cognitive} {Effort}?},
  url        = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0068210},
  urldate    = {2021-06-29},
}

